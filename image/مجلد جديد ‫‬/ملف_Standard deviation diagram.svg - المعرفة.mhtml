.mw-ui-button { background-color: rgb(248, 249, 250); color: rgb(34, 34, 34); display: inline-block; box-sizing: border-box; min-width: 4em; max-width: 28.75em; margin: 0px; padding: 0.571429em 0.9375em; border: 1px solid rgb(162, 169, 177); border-radius: 2px; font-family: inherit; font-size: 1em; font-weight: bold; line-height: 1; text-align: center; -webkit-appearance: none; zoom: 1; vertical-align: middle; cursor: pointer; }

.mw-ui-button:visited { color: rgb(34, 34, 34); }

.mw-ui-button:hover { background-color: rgb(255, 255, 255); color: rgb(68, 68, 68); border-color: rgb(162, 169, 177); }

.mw-ui-button:focus { background-color: rgb(255, 255, 255); color: rgb(34, 34, 34); border-color: rgb(51, 102, 204); box-shadow: rgb(51, 102, 204) 0px 0px 0px 1px inset, rgb(255, 255, 255) 0px 0px 0px 2px inset; outline-width: 0px; }

.mw-ui-button:active, .mw-ui-button.is-on { background-color: rgb(200, 204, 209); color: rgb(0, 0, 0); border-color: rgb(114, 119, 125); box-shadow: none; }

.mw-ui-button:disabled { background-color: rgb(200, 204, 209); color: rgb(255, 255, 255); border-color: rgb(200, 204, 209); cursor: default; }

